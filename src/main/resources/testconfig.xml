                                    <path>/${accountId}/domains/${id}?showRecords=false&amp;showSubdomains=false</path>
                                    <filters>
                                        <filter>recordsList</filter>
										<filter>subdomains</filter>
                                    </filters>
                                    <responseBody />
                                </output>
                                
								<!-- dns-list-domain-details-with-records-no-subdomains-request.json -->
