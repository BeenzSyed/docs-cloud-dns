								<!-- dns-list-domain-details-with-records-no-subdomains-response.json -->
								<!-- dns-list-domain-details-with-records-no-subdomains-response.xml -->
                                <output>
                                    <nameBase>dns-list-domain-details-with-records-no-subdomains</nameBase>
                                    <base>domains[0]</base>
                                    <path>/${accountId}/domains/${id}?showRecords=true&amp;showSubdomains=false</path>
                                    <filters>
										<filter>subdomains</filter>
                                    </filters>
                                    <responseBody />
                                </output>


								<!-- dns-list-domains-request.json -->
								<!-- dns-list-domains-request.xml -->
								<!-- dns-list-domains-response.json -->
								<!-- dns-list-domains-response.xml -->
								<!-- https://dns.api.rackspacecloud.com/v1.0/1234/domains?limit=10&offset=20 -->
                                <output>
                                    <nameBase>dns-list-domains</nameBase>
                                    <path>/${domains[0].accountId}/domains?limit=10&amp;offset=20</path>
                                    <filters>
                                    	<filter>domains[0].nameservers</filter>
                                    	<filter>domains[0].recordsList</filter>
										<filter>domains[0].subdomains</filter>
                                    </filters>
                                    <responseBody>
