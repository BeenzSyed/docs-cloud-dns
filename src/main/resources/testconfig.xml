                                        	<filter>domains[0].accountId</filter>
                                            <filter>domains[0].id</filter>
                                            <filter>domains[0].updated</filter>
                                            <filter>domains[0].created</filter>
                                            <filter>domains[0].recordsList.totalEntries</filter>
                                            <filter>domains[0].recordsList.records.id</filter>
                                            <filter>domains[0].recordsList.records.updated</filter>
                                            <filter>domains[0].recordsList.records.created</filter>
                                            <filter>domains[0].subdomains.totalEntries</filter>
                                            <filter>domains[0].subdomains.domains.id</filter>
                                            <filter>domains[0].subdomains.domains.nameservers</filter>
                                            <filter>domains[0].subdomains.domains.updated</filter>
                                            <filter>domains[0].subdomains.domains.created</filter>
                                            <filter>domains[9]</filter>
                                            <filter>domains[8]</filter>
                                            <filter>domains[7]</filter>
                                            <filter>domains[6]</filter>
                                            <filter>domains[5]</filter>
                                            <filter>domains[4]</filter>
                                            <filter>domains[3]</filter>
                                            <filter>domains[2]</filter>
                                            <filter>domains[1]</filter>
                                            <filter>links</filter>
