                                	<requestType>DELETE</requestType>
                                    <statusCode>202</statusCode>
                                </output>                    
                    
                    
								<!-- dns-remove-records-request.json -->
								<!-- dns-remove-records-request.xml -->
                                <output>
                                	<nameBase>dns-remove-records</nameBase>
                                	<base>domains[0]</base>
