                                    <responseBody>
										<base>domains[0]</base>
                                    </responseBody>
                                </output>
                                
								<!-- dns-create-domains-request.json -->
								<!-- dns-create-domains-request.xml -->
								<!-- dns-create-domains-response.json -->
								<!-- dns-create-domains-response.xml -->
