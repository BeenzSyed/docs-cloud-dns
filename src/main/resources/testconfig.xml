<project>
    <build>
        <plugins>
            <plugin>
                <groupId>com.rackspace.cloud.dns.maven.plugin</groupId>
                <artifactId>generate-samples-maven-plugin</artifactId>
                <version>0.0.1-SNAPSHOT</version>
                <configuration>
                    <namespace>http://docs.rackspacecloud.com/dns/api/v1.0</namespace>
                    <displayResults>true</displayResults>
                    <modules>
                        <module implementation="com.rackspace.cloud.dns.api.common.helpers.CommonJsonModule" />
                        <module implementation="com.rackspace.cloud.dns.helpers.PublicJsonModule" />
                    </modules>
                    <outputDir>${basedir}/target/samples</outputDir>
                    <urlBase>https://dns.api.rackspacecloud.com/v1.0</urlBase>
                    <generateJson>true</generateJson>
                    <generateXml>true</generateXml>
                    <requestHeaders>
                        <requestHeader>X-Auth-Token: ea85e6ac-baff-4a6c-bf43-848020ea3812</requestHeader>
                    </requestHeaders>
                    <responseHeaders>
                        <responseHeader>X-API-VERSION: 1.0.8</responseHeader>
                    </responseHeaders>
                    <sources>
                    
 						                       
                       
                       	<!--+++++++++++++++++++++++++++++++++++++++++++++++++++-->
						<!-- API BEANS OUTPUTS - domains, domain, links etc    -->
						<!--+++++++++++++++++++++++++++++++++++++++++++++++++++-->                   
                        <source>
                            <data>${basedir}/src/test/resources/samples.xml</data>
                            <beanPackage>com.rackspace.cloud.dns.api.beans</beanPackage>
