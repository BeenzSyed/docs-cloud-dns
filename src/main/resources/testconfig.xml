								<!-- dns-add-records-response.json -->
								<!-- dns-add-records-response.xml -->
                                <output>
                                    <nameBase>dns-add-records</nameBase>
                                    <base>domains[0]</base>
                                    <path>/${accountId}/domains/${id}/records</path>
                                    <requestType>POST</requestType>
                                    <statusCode>202</statusCode>
                                    <filters>
                                        <filter>accountId</filter>
                                    </filters>
                                    <requestBody>
                                        <base>recordsList</base>
                                        <filters>
                                            <filter>totalEntries</filter>
                                            <filter>records.id</filter>
                                            <filter>records.updated</filter>
                                            <filter>records.created</filter>
                                        </filters>
                                    </requestBody>
                                    <responseBody>
                                        <base>recordsList</base>                                
                                    </responseBody>
                                </output>
 
                                <!-- dns-list-domain-details-no-records-no-subdomains-request.json -->
