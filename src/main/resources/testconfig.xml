								<!-- dns-list-records-response.json -->
								<!-- dns-list-records-response.xml -->
                                <output>
                                    <nameBase>dns-list-records</nameBase>
                                    <base>domains[0]</base>
                                    <path>/${accountId}/domains/${id}/records</path>
                                    <filters>
                                    	<filter>nameservers</filter>
										<filter>subdomains</filter>
                                    </filters>
                                    <responseBody>
                                    	<base>recordsList</base>
                                    </responseBody>
                                  
                                </output>		
		

 
