                                    <nameBase>dns-list-record-details</nameBase>
                                    <base>domains[0]</base>
                                    <path>/${accountId}/domains/${id}/records/${recordsList.records[1].id}</path>
									<responseBody>
										<base>recordsList.records[1]</base>
									</responseBody>
                                </output>
                                
